{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": "nextjs",
  "buildCommand": "cd frontend && npm run build",
  "outputDirectory": "frontend/dist",
  "installCommand": "cd frontend && npm install",
  "rewrites": [
    {
      "source": "/creators/:id",
      "destination": "/creators/:id.html"
    },
    {
      "source": "/explore",
      "destination": "/explore.html"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
